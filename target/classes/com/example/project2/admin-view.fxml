<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TreeItem?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.GridPane?>
<HBox alignment="CENTER_LEFT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.project2.DriverController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TreeView></TreeView>
    <HBox alignment="TOP_RIGHT" spacing="20.0">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </padding>
        <TextField></TextField>
        <VBox alignment="TOP_RIGHT" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Button text="Add User" onAction="#onAddUser"/>
            <Button text="Add Group" onAction="#onAddGroup"/>
            <Button text="Open User View" onAction="#onOpenUserView"/>
            <Button text="Show User Total" onAction="#onShowUserTotal"/>
            <Button text="Show Group Total" onAction="#onShowGroupTotal"/>
            <Button text="Show Messages Total" onAction="#onShowMessageTotal"/>
            <Button text="Show Positive Percentage" onAction="#onShowPositivePercentage"/>
            <Label fx:id="labelText"/>
            </VBox>
        </HBox>
</HBox>
